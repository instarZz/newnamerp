<form action="{{path('app_profil_delete', {'id': app.user.id})}}" onsubmit="return confirm('Confirmer la suppression?')" method="POST">
    <input type="hidden"  name="_method" value="DELETE">
    <input type="hidden" name="csrf_token" value="{{ csrf_token('delete' ~ app.user.id) }}">
    <input type="submit" value="Supprimer">
</form>
